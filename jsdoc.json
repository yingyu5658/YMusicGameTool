{
  "source": {
    "include": ["./src/"]
  },
  "opts": {
    "template": "./node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "theme_opts": {
      "default_theme": "dark"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
